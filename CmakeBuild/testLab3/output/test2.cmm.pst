FUNCTION fact :
PARAM p1
IF p1 == #1 GOTO label$0_body
GOTO label$1_else
LABEL label$0_body :
RETURN p1
GOTO label$2_after
LABEL label$1_else :
t3 := p1 - #1
ARG t3
t2 := CALL fact
